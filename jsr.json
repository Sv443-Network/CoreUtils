{
  "$schema": "https://jsr.io/schema/config-file.v1.json",
  "name": "@sv443-network/coreutils",
  "version": "1.0.0",
  "exports": {
    ".": "./lib/index.ts"
  },
  "publish": {
    "include": [
      "./dist/CoreUtils.cjs",
      "./dist/CoreUtils.min.cjs",
      "./dist/CoreUtils.mjs",
      "./dist/CoreUtils.min.mjs",
      "./dist/CoreUtils.umd.js",
      "./dist/CoreUtils.min.umd.js",
      "./dist/lib/**/*.d.ts",
      "./lib/**/*.ts",
      "./package.json",
      "./README.md",
      "./CHANGELOG.md",
      "./LICENSE.txt"
    ],
    "exclude": [
      "./**/*.spec.ts"
    ]
  }
}
